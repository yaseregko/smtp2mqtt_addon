{
  "name": "smtp2mqtt",
  "version": "dev",
  "slug": "example",
  "description": "smtp2mqtt add-on by Community Home Assistant Add-ons",
  "url": "https://github.com/yaseregko/smtp2mqtt",
  "startup": "application",
  "init": false,
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "options": {
    "mqtt": "info",
    "seconds_between_quotes": 5
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "seconds_between_quotes": "int(1,120)"
  }
}
